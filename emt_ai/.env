# one of: transformers | ollama | vllm
LLM_BACKEND=transformers

# transformers local model for your Inspiron (small & fast)
TRANSFORMERS_MODEL=TinyLlama/TinyLlama-1.1B-Chat-v1.0

# ollama (if you install it locally)
OLLAMA_MODEL=mistral

# vLLM server (cloud) when youâ€™re ready to switch to gpt-oss-20b
VLLM_ENDPOINT=http://<YOUR-CLOUD-IP>:8000/v1
VLLM_MODEL=openai/gpt-oss-20b
